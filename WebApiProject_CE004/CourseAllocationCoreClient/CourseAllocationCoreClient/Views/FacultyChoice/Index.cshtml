@model IEnumerable<CourseAllocationCoreClient.Models.FacultyChoice>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Priority1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Priority2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Priority3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Priority4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Priority5)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Priority5)
            </td>
          
        </tr>
}
    </tbody>
</table>
